<?xml version="1.0" encoding="utf-8"?>
<view:PNGButtonPaneView xmlns:view="edu.wisc.doit.ls.coolit.view.*" xmlns:mx="http://www.adobe.com/2006/mxml"
	horizontalAlign="center" verticalAlign="middle">
	
	<mx:ViewStack id="imageStack" width="100%" height="100%" creationPolicy="all">
		<mx:Box width="100%" height="100%"><mx:Image id="imageUp" source="{upImage}" width="100%" height="100%"
			verticalAlign="middle" horizontalAlign="center" /></mx:Box>
		<mx:Box width="100%" height="100%"><mx:Image id="imageOver" source="{overImage}" width="100%" height="100%"
				verticalAlign="middle" horizontalAlign="center" /></mx:Box>
		<mx:Box width="100%" height="100%"><mx:Image id="imageDown" source="{downImage}" width="100%" height="100%"
				verticalAlign="middle" horizontalAlign="center" /></mx:Box>
	</mx:ViewStack>
	
</view:PNGButtonPaneView>