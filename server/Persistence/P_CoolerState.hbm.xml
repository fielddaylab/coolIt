<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Persistence.P_CoolerState, Persistence" table="CoolerStates">
		<id name="Id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="InputPower" column="inputPower" />
    <property name="PowerFactor" column="powerFactor" />
		<many-to-one
				  name="Cooler"
				  column="coolerId"
				  class="Persistence.P_Cooler, Persistence"
				  not-null="true"
				  cascade="all"
		/>
    <many-to-one
				  name="ProblemState"
				  column="problemStateId"
				  class="Persistence.P_ProblemState, Persistence"
				  not-null="true"
				  cascade="all"
		/>
	</class>
</hibernate-mapping>
