<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Persistence.P_StrutState, Persistence" table="StrutStates">
		<id name="Id" column="id" type="integer">
			<generator class="identity" />
		</id>
		<property name="Length" column="length" />
		<property name="CrossSection" column="crossSection" />
    <property name="Strength" column="strength" />
		<many-to-one
				  name="Material"
				  column="materialId"
				  class="Persistence.P_Material, Persistence"
				  not-null="true"
				  cascade="all"
		/>
    <many-to-one
				  name="ProblemState"
				  column="problemStateId"
				  class="Persistence.P_ProblemState, Persistence"
				  not-null="true"
				  cascade="all"
		/>
	</class>
</hibernate-mapping>
